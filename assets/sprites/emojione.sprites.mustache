{
  // Default options
  'functions': true
}

.emojione {
  text-indent: -9999em;
  image-rendering: optimizeQuality;
  font-size: inherit;
  height: 20px;
  width: 20px;
  position: relative;
  display: inline-block;
  margin: 0;
  line-height: normal;
  vertical-align: middle;
  background-image: url('../images/emojione.sprites.png');
  background-repeat: no-repeat;
  background-size: 986px 986px;
}

{{#items}}
.{{name}} {
  background-position: {{position_x}}px {{position_y}}px;
}
{{/items}}
